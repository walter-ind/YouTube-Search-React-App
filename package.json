  },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^4.17.13",
    "react": "16.3.2",
    "react-dom": "16.3.3",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "webpack": "^3.10.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^2.9.7",
    "youtube-api-search": "0.0.5"
